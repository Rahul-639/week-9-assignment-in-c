
def countDistinctInWindow(arr, k):
    n = len(arr)
    if k > n:
        return []
    freq = defaultdict(int)  
    result = []
    for i in range(k):
        freq[arr[i]] += 1
    result.append(len(freq))
    for i in range(k, n):
        outgoing = arr[i - k]
        freq[outgoing] -= 1
        if freq[outgoing] == 0:
            del freq[outgoing]
        freq[arr[i]] += 1
        result.append(len(freq))

    return result

